<?xml version="1.0"?>
<program vert="glsl/sky-water.vert"
         frag="glsl/sky-water.frag" >
  <sampler name="fill"  unit="0" />
  <sampler name="glow"  unit="1" />
  <sampler name="norm"  unit="2" />

  <uniform name="light_position" value="light_position" size="3"/>
  <uniform name="time"           value="time"           size="1"/>
  <uniform name="view_position"  value="view_position"  size="3"/>
</program>
